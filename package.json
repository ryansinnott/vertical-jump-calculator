{
  "name": "verticle-jump-calculator",
  "version": "1.0.0",
  "description": "A mobile web app that measures your vertical jump height from video using physics-based calculation.",
  "main": "app.js",
  "scripts": {
    "build": "node scripts/build.js",
    "build:icons": "node scripts/generate-icons.js",
    "cap:sync": "npm run build && npx cap sync",
    "cap:android": "npm run build && npx cap sync android && npx cap open android",
    "cap:ios": "npm run build && npx cap sync ios && npx cap open ios",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryansinnott/vertical-jump-calculator.git"
  },
  "keywords": [],
