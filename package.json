  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/ryansinnott/vertical-jump-calculator/issues"
  },
  "homepage": "https://github.com/ryansinnott/vertical-jump-calculator#readme",
  "devDependencies": {
    "sharp": "^0.34.5"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.0",
    "@capacitor/cli": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@capacitor/ios": "^8.0.0",
    "tslib": "^2.8.1"
  }
}
